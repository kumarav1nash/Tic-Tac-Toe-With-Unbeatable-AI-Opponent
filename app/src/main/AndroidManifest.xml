<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.virgindroid.tictac">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icon_tictac_circle_minified"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_icon_tictac_circle_minified"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTac">
        <activity android:name=".GameBoardActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>